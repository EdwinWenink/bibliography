  author      = {Meyer-Vitali, A. P. and Bakker, R. M. and van Bekkum, M. A. and de Boer, M. and Burghouts, G. and van Diggelen, J. and Dijk, J. and Grappiolo, C. and de Greeff, J. and Huizing, A. and Raaijmakers, S.},
